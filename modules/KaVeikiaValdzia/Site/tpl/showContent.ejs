var side;
var html;
var breadcrumbs;

var salt = dates.format(new Date(), "yyyyMMdd");


function css()
{
    var html = "";
    for (var i=0; i<arguments.length; i++)
        html += '<link rel="stylesheet" type="text/css" href="' + config.FILES_URL + '/' + arguments[i] + '.css?' + salt + '">';
    return html;
}

function javascript()
{
    var html = "";
    for (var i=0; i<arguments.length; i++)
        html += '<script type="text/javascript" src="' + config.FILES_URL + '/' + arguments[i] + '.js?' + salt + '"></script>';
    return html;
}


